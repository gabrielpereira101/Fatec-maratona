#include <stdio.h>
 
int main() {
 
    double valor;
    int n1, n2, n3, n4, n5, n6;
    int m1, m2, m3, m4, m5, m6;

    scanf("%lf", &valor);
    
    printf("NOTAS:\n");

    n1 = valor/100;
    printf("%d nota(s) de R$ 100.00\n", n1);
    valor -= n1 * 100;
    
    n2 = valor/50;
    printf("%d nota(s) de R$ 50.00\n", n2);
    valor -= n2 * 50;
    
    n3 = valor/20;
    printf("%d nota(s) de R$ 20.00\n", n3);
    valor -= n3 * 20;
    
    n4 = valor/10;
    printf("%d nota(s) de R$ 10.00\n", n4);
    valor -= n4 * 10;
    
    n5 = valor/5;
    printf("%d nota(s) de R$ 5.00\n", n5);
    valor -= n5 * 5;
    
    n6 = valor/2;
    printf("%d nota(s) de R$ 2.00\n", n6);
    valor -= n6 * 2;

    printf("MOEDAS:\n");

    m1 = valor/1;
    printf("%d moeda(s) de R$ 1.00\n", m1);
    valor -= m1 * 1;

    m2 = valor/0.50;
    printf("%d moeda(s) de R$ 0.50\n", m2);
    valor -= m2 * 0.50;
    
    m3 = valor/0.25;
    printf("%d moeda(s) de R$ 0.25\n", m3);
    valor -= m3 * 0.25;
    
    m4 = valor/0.10;
    printf("%d moeda(s) de R$ 0.10\n", m4);
    valor -= m4 * 0.10;
    
    m5 = valor/0.05;
    printf("%d moeda(s) de R$ 0.05\n", m5);
    valor -= m5 * 0.05;
    
    m6 = valor/0.01;
    printf("%d moeda(s) de R$ 0.01\n", m6);
    valor -= m6 * 0.01;
    
 
    return 0;
}
